{
  "packageManager": "yarn@4.0.1",
  "homepage": "https://mujaen.github.io/beanpod",
  "scripts": {
    "dev": "NODE_ENV=development DIST_ENV=development PROTOCOL=https node server/index.js",
    "start": "NODE_ENV=development DIST_ENV=development PROTOCOL=https node server/index.js",
    "start:prod": "NODE_ENV=development DIST_ENV=production PROTOCOL=https node server/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "yarn run typecheck && yarn run lint:eslint:quiet .",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern",
    "lint:eslint:quiet": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --quiet",
    "lint:eslint:fix": "eslint --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern --quiet --fix",
    "build": "NODE_ENV=production DIST_ENV=production webpack --config webpack/webpack.prod.config.js --progress",
    "build:clean": "rimraf ./build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
